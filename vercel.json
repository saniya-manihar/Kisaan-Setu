{
  "version": 2,
  "builds": [
    {
      "src": "kisan/Kisaan_Setu/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "kisan/Kisaan_Setu/$1" }
  ],
  "framework": "create-react-app",
  "outputDirectory": "kisan/Kisaan_Setu/build",
  "buildCommand": "npm run build",
  "installCommand": "npm install"
}
